<command>
    <name>Turn OFF relay on UDOO DomuShield</name>
    <receiver>app.actuators.protocol.udoo-domushield.in</receiver>
    <description>Turns OFF relay1 on UDOO DomuShield</description>
    <hardwareLevel>true</hardwareLevel>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="udoo-domushield.command" value="TURN_OFF_RELAY"/>
            <property name="udoo-domushield.idCode" value="@owner.object.address"/>
        </properties>
    </properties>
</command>
