<com.freedomotic.reactions.Command>
    <name>Turn OFF Philips Hue light</name>
    <receiver>app.actuators.protocol.philips-hue.in</receiver>
    <description>turns off the Philips Hue light</description>
    <hardwareLevel>true</hardwareLevel>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="command" value="off"/>
            <property name="address" value="@owner.object.address"/>
        </properties>
    </properties>
</com.freedomotic.reactions.Command>
