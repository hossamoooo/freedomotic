<command>
    <name>Authometion Shield: Set RGB green value</name>
    <receiver>app.actuators.protocol.authometion-shield.in</receiver>
    <description>set the green value of an Authometion LYT/RGBW light</description>
    <hardwareLevel>true</hardwareLevel>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="authometion.command" value="RGB"/>
            <property name="address" value="@owner.object.address"/>
            <property name="red" value="@owner.object.behavior.red"/>
            <property name="green" value="@value"/>
            <property name="blue" value="@owner.object.behavior.blue"/>
        </properties>
    </properties>
</command>
